<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>single-service-projectId</application><!-- overriden in buildfiles -->
  <version>single-service-version</version><!-- overriden in buildfiles -->
  <service>single-service</service>

  <!-- java 8 options -->
  <runtime>java8</runtime>
  <threadsafe>true</threadsafe>

  <!-- java 11 options
  <runtime>java11</runtime>
  -->

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>
</appengine-web-app>
